<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<!DOCTYPE window SYSTEM "chrome://downloaddialogtweak/locale/options.dtd">

<prefwindow
    id="downloaddialogtweak"
    xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
<prefpane id="main" flex="1">

    <preferences>
        <preference id="sendto" type="string"
                    name="extensions.downloaddialogtweak.sendto"/>
    </preferences>

    <label value="&sendto;" />
    <textbox flex="1" multiline="true" wrap="off" rows="12" cols="54"
             style="font-family: 'DejaVu Sans Mono',monospace;"
             preference="sendto" />

</prefpane>
</prefwindow>
